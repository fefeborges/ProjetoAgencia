@model ProjetoAgencia.Models.Cadastro

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <h4>Cadastro</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomePessoa)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomePessoa)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Destino)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Destino.NomeDestino)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Estadia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Estadia.NomeEstadia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Passageiros)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Passageiros.NomePassageiros)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Transporte)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Transporte.NomeTransporte)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Atracoes)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Atracoes.NomeAtracoes)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Contato)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Contato)
        </dd>
    </dl>
</div>
<div>
    <a class="a" asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a class="a" asp-action="Index">Voltar para a lista</a>
</div>
